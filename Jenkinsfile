pipeline{
    agent any {
            stages{
                stage ("build"){
                    steps {
                        echo "Build the application app_name"
                }
                stage ("test"){
                    steps {
                        echo "Testing the application app_name"
                }
                stage ("deploy"){
                    steps {
                        echo "Deploying the application app_name"
                }
            }
        }
    }
}